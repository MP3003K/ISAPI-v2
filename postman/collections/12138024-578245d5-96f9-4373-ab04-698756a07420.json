			"name": "1. Login",
			"item": [
				{
					"name": "1.1 capabilities",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "c1cfb5e5-20a3-49e5-8aac-ef5e9ad2e03a",
								"exec": [
									"/**\r",
									" * REQUISITOS:\r",
									" * - Variables globales definidas: \r",
									" *    - 'ip' (IP del dispositivo)\r",
									" *    - 'usuario' (nombre de usuario)\r",
									" *    - 'contrase√±a' (contrase√±a en texto plano)\r",
									" */\r",
									"\r",
									"try {\r",
									"    // Obtener el XML response del request capabilities\r",
									"    let xml = pm.response.text();\r",
									"\r",
									"    // Convertir XML a JSON con xml2Json (funci√≥n de Postman)\r",
									"    let json = xml2Json(xml);\r",
									"    console.log(\"XML convertido a JSON:\", json);\r",
									"\r",
									"    // Extraer el nodo SessionLoginCap\r",
									"    let data = json?.SessionLoginCap;\r",
									"    if (!data) {\r",
									"        console.error(\"‚ùå No se encontr√≥ el nodo SessionLoginCap.\");\r",
									"        return;\r",
									"    }\r",
									"\r",
									"    // Extraer datos necesarios\r",
									"    let salt = data.salt;\r",
									"    let iterations = parseInt(data.iterations, 10);\r",
									"    let sessionID = data.sessionID;\r",
									"    let sessionIDVersion = data.sessionIDVersion || \"3\";\r",
									"\r",
									"    if (!salt || !iterations || !sessionID) {\r",
									"        console.error(\"‚ùå Faltan datos necesarios (salt, iterations, sessionID).\");\r",
									"        return;\r",
									"    }\r",
									"\r",
									"    // Extraer isNeedSessionTag desde isSupportSessionTag (puede venir como objeto o string)\r",
									"    let isSupportSessionTagRaw = data.isSupportSessionTag;\r",
									"    let isNeedSessionTag;\r",
									"\r",
									"    if (typeof isSupportSessionTagRaw === \"object\") {\r",
									"        if (\"#text\" in isSupportSessionTagRaw) {\r",
									"            isNeedSessionTag = isSupportSessionTagRaw[\"#text\"];\r",
									"        } else {\r",
									"            isNeedSessionTag = isSupportSessionTagRaw.toString();\r",
									"        }\r",
									"    } else {\r",
									"        isNeedSessionTag = isSupportSessionTagRaw;\r",
									"    }\r",
									"\r",
									"    isNeedSessionTag = isNeedSessionTag.toString().toLowerCase() === \"true\";\r",
									"\r",
									"    // Obtener usuario y contrase√±a de variables globales\r",
									"    let usuario = pm.globals.get(\"usuario\");\r",
									"    let contrase√±a = pm.globals.get(\"contrase√±a\");\r",
									"\r",
									"    if (!usuario || !contrase√±a) {\r",
									"        console.error(\"‚ùå Las variables globales 'usuario' o 'contrase√±a' no est√°n definidas.\");\r",
									"        return;\r",
									"    }\r",
									"\r",
									"    // Hashear la contrase√±a con PBKDF2 usando CryptoJS\r",
									"    // La salt debe convertirse de hexadecimal a un WordArray\r",
									"    let hashedPassword = CryptoJS.PBKDF2(\r",
									"        contrase√±a,\r",
									"        CryptoJS.enc.Hex.parse(salt),\r",
									"        {\r",
									"            keySize: 256 / 32,  // 256 bits / 32 bytes\r",
									"            iterations: iterations,\r",
									"            hasher: CryptoJS.algo.SHA256\r",
									"        }\r",
									"    ).toString();\r",
									"\r",
									"    console.log(\"‚úÖ Contrase√±a hasheada:\", hashedPassword);\r",
									"\r",
									"    // Construir XML para el login\r",
									"    let loginXML = `\r",
									"<SessionLogin>\r",
									"    <userName>${usuario}</userName>\r",
									"    <password>${hashedPassword}</password>\r",
									"    <sessionID>${sessionID}</sessionID>\r",
									"    <isSessionIDValidLongTerm>falsez</isSessionIDValidLongTerm>\r",
									"    <sessionIDVersion>${sessionIDVersion}</sessionIDVersion>\r",
									"    <isNeedSessionTag>${isNeedSessionTag}</isNeedSessionTag>\r",
									"</SessionLogin>`.trim();\r",
									"\r",
									"    // Guardar XML completo en variable global para usar en el request login POST\r",
									"    pm.globals.set(\"loginXML\", loginXML);\r",
									"\r",
									"    // Guardar otros datos √∫tiles tambi√©n globales\r",
									"    pm.globals.set(\"sessionID\", sessionID);\r",
									"    pm.globals.set(\"sessionIDVersion\", sessionIDVersion);\r",
									"    pm.globals.set(\"isNeedSessionTag\", isNeedSessionTag.toString());\r",
									"\r",
									"    console.log(\"‚úÖ Variables globales actualizadas con datos para login.\");\r",
									"    console.log(\"Login XML:\", loginXML);\r",
									"\r",
									"} catch (e) {\r",
									"    console.error(\"‚ùå Error en script de test:\", e.message);\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"id": "09709bcb-c75a-4030-9b69-c7d8fdf7cd51",
								"exec": [
									"/**\r",
									" * ‚ö†Ô∏è REQUISITOS EN VARIABLES GLOBALES:\r",
									" * - 'ip': IP del dispositivo\r",
									" * - 'usuario': Nombre de usuario administrador\r",
									" * - 'contrase√±a': Contrase√±a en texto plano\r",
									" *\r",
									" * üìå OBSERVACI√ìN IMPORTANTE:\r",
									" * Este script genera un n√∫mero aleatorio de 8 d√≠gitos (entero positivo) que ser√° √∫nico en cada ejecuci√≥n.\r",
									" * El n√∫mero se almacena en la variable global 'numeroRandom' y es utilizado como parte del par√°metro en la URL.\r",
									" */\r",
									"\r",
									"\r",
									"function generateUniqueRandom(lastNumber) {\r",
									"    let newNumber;\r",
									"    do {\r",
									"        newNumber = Math.floor(Math.random() * (99999999 - 10000000 + 1)) + 10000000;\r",
									"    } while (newNumber === lastNumber);\r",
									"    return newNumber;\r",
									"}\r",
									"\r",
									"// Obtener el √∫ltimo n√∫mero generado (puede ser null si es la primera vez)\r",
									"const lastNumber = pm.environment.get(\"lastNumeroRandom\");\r",
									"\r",
									"// Generar nuevo n√∫mero √∫nico\r",
									"const newRandomNumber = generateUniqueRandom(lastNumber);\r",
									"\r",
									"// Guardar nuevo n√∫mero en variable 'numeroRandom'\r",
									"pm.environment.set(\"numeroRandom\", newRandomNumber);\r",
									"\r",
									"// Actualizar el √∫ltimo n√∫mero generado para la pr√≥xima ejecuci√≥n\r",
									"pm.environment.set(\"lastNumeroRandom\", newRandomNumber);\r",
									"\r",
									"console.log(\"N√∫mero aleatorio generado √∫nico:\", newRandomNumber);\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
