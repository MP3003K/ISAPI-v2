						{
							"key": "JSON_SetFaceRecord",
							"value": "{\n  \"faceLibType\": \"blackFD\",\n  \"FDID\": \"1\",\n  \"FPID\": \"11\",\n  \"bornTime\": \"2001-03-30T00:00:00Z\",\n  \"employeeNo\": \"11\"\n}",
